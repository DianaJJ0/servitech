# Variables de Entorno para el Frontend ServiTech

# Permitir establecer sesiones en modo desarrollo
ALLOW_DEV_SET_SESSION=true

# Mostrar botones de desarrollo admin
SHOW_DEV_ADMIN=true

# Deshabilitar CSRF en desarrollo (solo para testing)
DISABLE_CSRF=true

# Puerto del frontend
PORT=3001

# URL del backend
BACKEND_URL=http://localhost:3000
